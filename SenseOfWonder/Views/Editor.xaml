<UserControl x:Class="SenseOfWonder.Views.Editor"
				 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
				 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
				 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
				 xmlns:SenseOfWonder="clr-namespace:SenseOfWonder"
				 mc:Ignorable="d" 
				 d:DesignHeight="300" d:DesignWidth="300">
	<Grid d:DataContext="{d:DesignInstance {x:Type SenseOfWonder:WonderCharacterDesignData}, IsDesignTimeCreatable=True}">
		<TextBox x:Name="Name" Height="23.2" Margin="22.6,30.4,71.8,0" TextWrapping="Wrap" Text="{Binding Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top"/>
		
	 </Grid>
</UserControl>
